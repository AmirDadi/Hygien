
<body>

    
        <div id="page-wrapper">
            <div class="row">
                <div class="col-lg-12">
                    <h1 class="page-header">Encounters</h1>
                </div>
                <!-- /.col-lg-12 -->
            </div>
            <!-- /.row -->
            <div class="row">
                <div class="col-lg-12">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            DataTables Advanced Tables
                        </div>
                        <!-- /.panel-heading -->
                        <div class="panel-body">
                            <div class="dataTable_wrapper">
                                <table class="table table-striped table-bordered table-hover" id="dataTables-example">
                                    <thead>
                                        
                                        <tr>
                                            <th> time </th>
                                            <th> status </th>
                                            <th> card id </th>
                                            <th> sensor id </th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                    <% @encounters.each do |encounter| %>
                                        <tr class="odd gradeX">
                                            <td> <%= encounter.time %> </td>
                                            <td> <%= encounter.status %> </td>
                                            <td> <%= encounter.card_cid %> </td>
                                            <td> <%= encounter.sensor_sid %> </td>
                                        </tr>
                                    <% end %>    
                                    </tbody>
                                </table>
                            </div>
                            <!-- /.table-responsive -->
               <!--              <div class="well">
                                <h4>DataTables Usage Information</h4>
                                <p>DataTables is a very flexible, advanced tables plugin for jQuery. In SB Admin, we are using a specialized version of DataTables built for Bootstrap 3. We have also customized the table headings to use Font Awesome icons in place of images. For complete documentation on DataTables, visit their website at <a target="_blank" href="https://datatables.net/">https://datatables.net/</a>.</p>
                                <a class="btn btn-default btn-lg btn-block" target="_blank" href="https://datatables.net/">View DataTables Documentation</a>
                            </div> -->
                        </div>
                        <!-- /.panel-body -->
                    </div>
                    <!-- /.panel -->
                </div>
                <!-- /.col-lg-12 -->
            </div>
            <!-- /.row -->
          
    <!-- /#wrapper -->


    <!-- jQuery -->

    <%= javascript_include_tag('bower_components/jquery/dist/jquery.min.js') %>
    

    <!-- Bootstrap Core JavaScript -->
    <%= javascript_include_tag('bower_components/bootstrap/dist/js/bootstrap.min.js') %>
    
    <!-- Metis Menu Plugin JavaScript -->
   

    <!-- DataTables JavaScript -->

    <%= javascript_include_tag('bower_components/datatables/media/js/jquery.dataTables.min.js') %>
    <%= javascript_include_tag('bower_components/datatables-plugins/integration/bootstrap/3/dataTables.bootstrap.min.js') %>
    <%= javascript_include_tag('bower_components/metisMenu/dist/metisMenu.min.js') %>
    <!-- Custom Theme JavaScript -->
    <%= javascript_include_tag ('dist/js/sb-admin-2.js') %>
        
    <!-- Page-Level Demo Scripts - Tables - Use for reference -->
    <script>
    $(document).ready(function() {
        $('#dataTables-example').DataTable({
                responsive: true
        });
    });
    </script>

</body>

</html>

